{
  "app": {
    "host": {
      "path": "./app"
    },
    "framework": [
      {
        "path": "modules",
        "version": "1.0.0",
        "groupId": "com.hezhihu.module",
        "dependencies": {
          "implementation": [
            "appFramework"
          ]
        },
        "modules": [
          {
            "id": "homemodule",
            "path": "home"
          },
          {
            "id": "loginmodule",
            "path": "login"
          },
          {
            "id": "settingmodule",
            "path": "setting"
          },
          {
            "id": "launchermodule",
            "path": "launcher"
          }
        ]
      },{
        "path": "frameworks",
        "version": "1.0.0",
        "groupId": "com.hezhihu.frameworks",
        "dependencies": {
          "api": [
            "appNetWork",
            "appUtils"
          ]
        },
        "modules": [
          {
            "id": "network",
            "path": "network",
            "dependencies": {
              "implementation": [
                "rxJava",
                "rxAndroid",
                "okHttp",
                "okHttpLogging",
                "rxJavaAdapter",
                "rxJavaGsonConverter"
              ]
            }
          },
          {
            "id": "utils",
            "path": "utils",
            "dependencies": {
              "api": [
                "utilCode"
              ]
            }
          }
        ]
      }
    ],
    "maven": {
      "appNetWork": "com.hezhihu.frameworks:network:1.0.0",
      "appUtils": "com.hezhihu.frameworks:utils:1.0.0",
      "appFramework": "com.hezhihu.frameworks:frameworks:1.0.0",

      "rxJava": "io.reactivex.rxjava3:rxjava:3.1.3",
      "rxAndroid": "io.reactivex.rxjava3:rxandroid:3.0.0",
      "okHttp": "com.squareup.okhttp3:okhttp:4.9.3",
      "okHttpLogging": "com.squareup.okhttp3:logging-interceptor:4.9.3",
      "retrofit": "com.squareup.retrofit2:retrofit:2.9.0",
      "rxJavaAdapter": "com.squareup.retrofit2:adapter-rxjava3:2.9.0",
      "rxJavaGsonConverter": "com.squareup.retrofit2:converter-gson:2.9.0",
      "utilCode": "com.blankj:utilcodex:1.30.6",
      "gson": "com.google.code.gson:gson:2.8.9"
    }
  }
}