{
  "app": {
    "host": {
      "path": "./app"
    },
    "framework": [
      {
        "path": "gradle_plugin",
        "version": "1.0.0",
        "group": "com.hezhihu.gradle.plugin",
        "id": "gradle",
        "modules": [
          {
            "id": "setting",
            "path": "setting"
          },{
            "id": "apply-maven",
            "path": "applyMaven"
          },{
            "id": "dependencies",
            "path": "dependencies"
          },{
            "id": "base",
            "path": "base"
          }
        ]
      },
      {
        "path": "modules",
        "version": "1.0.0",
        "id": "modules",
        "group": "com.hezhihu.module",
        "modules": [
          {
            "id": "homemodule",
            "path": "home",
            "dependencies": {
              "implementation": [
                "appFramework"
              ]
            }
          },
          {
            "id": "loginmodule",
            "path": "login",
            "dependencies": {
              "implementation": [
                "appFramework"
              ]
            }
          },
          {
            "id": "settingmodule",
            "path": "setting",
            "dependencies": {
              "implementation": [
                "appFramework"
              ]
            }
          },
          {
            "id": "launchermodule",
            "path": "launcher",
            "dependencies": {
              "implementation": [
                "appFramework"
              ]
            }
          }
        ]
      },{
        "path": "frameworks",
        "version": "1.0.0",
        "id": "frameworks",
        "group": "com.hezhihu.framework",
        "dependencies": {
          "api": [
            "appNetWork",
            "appUtils"
          ]
        },
        "modules": [
          {
            "id": "network",
            "path": "network",
            "dependencies": {
              "implementation": [
                "rxJava",
                "rxAndroid",
                "okHttp",
                "okHttpLogging",
                "rxJavaAdapter",
                "rxJavaGsonConverter"
              ]
            }
          },
          {
            "id": "utils",
            "path": "utils",
            "dependencies": {
              "api": [
                "utilCode"
              ]
            }
          }
        ]
      }
    ]
  },
  "maven": {
    "appNetWork": "com.hezhihu.framework:network:1.0.0",
    "appUtils": "com.hezhihu.framework:utils:1.0.0",
    "appFramework": "com.hezhihu.framework:frameworks:1.0.0",

    "rxJava": "io.reactivex.rxjava3:rxjava:3.1.3",
    "rxAndroid": "io.reactivex.rxjava3:rxandroid:3.0.0",
    "okHttp": "com.squareup.okhttp3:okhttp:4.9.3",
    "okHttpLogging": "com.squareup.okhttp3:logging-interceptor:4.9.3",
    "retrofit": "com.squareup.retrofit2:retrofit:2.9.0",
    "rxJavaAdapter": "com.squareup.retrofit2:adapter-rxjava3:2.9.0",
    "rxJavaGsonConverter": "com.squareup.retrofit2:converter-gson:2.9.0",
    "utilCode": "com.blankj:utilcodex:1.30.6",
    "gson": "com.google.code.gson:gson:2.8.9"
  }
}